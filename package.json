    "eslint-config-atom-build": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.0",
    "gulp-jsonlint": "^1.1.2",
    "gulp-recess": "^1.2.0"
  },
  "scripts": {
    "test": "gulp lint"
  }
