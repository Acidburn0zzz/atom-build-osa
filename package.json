{
  "name": "build-osa",
  "version": "0.5.1",
  "description": "Atom Build provider for Apple's Open Scripting Architecture, runs or compiles AppleScript and JavaScript for Automation (JXA)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-build-osa.git"
  },
  "license": "MIT",
  "keywords": [
    "applescript",
    "build",
    "osx",
    "javascript for automation",
    "jxa"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "builder": {
      "description": "Runs or compiles AppleScript and JXA",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-package-deps": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
